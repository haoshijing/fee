<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yingliguoji.fee.dao.MemberMapper">

    <sql id="tableName">
        members
    </sql>
    <!-- 插入一条记录 -->

    <select id = "selectAll" resultType="com.yingliguoji.fee.po.MemberPo" >
        select id from  <include refid="tableName"/>

    </select>

    <select id = "selectById" resultType="com.yingliguoji.fee.po.MemberPo">
        select * from <include refid="tableName"/>
        where id = #{id}
    </select>

</mapper>