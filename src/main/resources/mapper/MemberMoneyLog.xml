<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yingliguoji.fee.dao.MemberMoneyLogMapper">

    <sql id="tableName">
        members
    </sql>
    <!-- 插入一条记录 -->

    <select id = "selectAll" resultType="com.yingliguoji.fee.po.MemberMoneyLog" >
        select id from  <include refid="tableName"/>

    </select>


    <insert id="insert" parameterType="com.yingliguoji.fee.po.MemberMoneyLog">

        insert into <include refid="tableName"/>
        (
        memberId,
        money
        type,
        memo,
        beforeMoney,
        afterMoney,
        createTime
        )

        VALUES (

        #{memberId},
        #{money},
        #{type},
        #{memo},
        #{beforeMoney},
        #{afterMoney},
        #{createTime}
        )

    </insert>

</mapper>