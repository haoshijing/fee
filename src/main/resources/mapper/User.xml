<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yingliguoji.fee.dao.UserMapper">

    <sql id="tableName">
        game_record
    </sql>
    <insert id="insert" parameterType="com.yingliguoji.fee.po.UserPo" useGeneratedKeys="true" keyProperty="id">
        Insert into
        <include refid="tableName"/>
        (
        name,
        password,
        isSuperAdmin,
        roleId,
        rememberToken,
        createdAt,
        updatedAt,
        invitation
        )
        VALUES
        (
        #{name},
        #{password},
        #{isSuperAdmin},
        #{roleId},
        #{rememberToken},
        #{createdAt},
        #{updatedAt},
        #{invitation}
        )

    </insert>


</mapper>